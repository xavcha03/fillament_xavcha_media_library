{
    "name": "xavcha/fillament-xavcha-media-library",
    "description": "Package Laravel/Filament complet pour gérer les médias avec support des conversions, collections, et intégration native Filament v4",
    "type": "library",
    "version": "1.1.0",
    "license": "MIT",
    "keywords": [
        "laravel",
        "filament",
        "media",
        "library",
        "upload",
        "images",
        "videos",
        "documents"
    ],
    "authors": [
        {
            "name": "Xavier"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "filament/filament": "^4.0",
        "livewire/livewire": "^3.0",
        "spatie/image-optimizer": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Xavier\\MediaLibraryPro\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xavier\\MediaLibraryPro\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xavier\\MediaLibraryPro\\Providers\\MediaLibraryProServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

